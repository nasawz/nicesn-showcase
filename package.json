{
  "name": "thinkjs-application",
  "description": "application created by thinkjs",
  "version": "1.0.0",
  "scripts": {
    "start": "node www/development.js",
    "compile": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/ --out-dir app/ --retain-lines",
    "watch-compile": "node -e \"console.log('<npm run watch-compile> no longer need, use <npm start> command direct.');console.log();\"",
    "watch": "npm run watch-compile",
    "web": "webpack-dev-server --devtool eval --progress --hot --colors --host 0.0.0.0 --content-base www/static/src/"
  },
  "dependencies": {
    "babel-runtime": "^6.3.19",
    "classnames": "^2.2.3",
    "invariant": "^2.2.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-prop-types": "^0.3.0",
    "thinkjs": "2.1.x",
    "warning": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "6.x.x",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "browser-sync": "^2.11.0",
    "chalk": "^1.1.1",
    "del": "^2.2.0",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.1",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-less": "^3.0.5",
    "gulp-load-plugins": "^1.2.0",
    "gulp-minify-css": "^1.2.3",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "merge-stream": "^1.0.0",
    "minimist": "^1.2.0",
    "run-sequence": "^1.1.5",
    "webpack": "^1.12.11"
  }
}
